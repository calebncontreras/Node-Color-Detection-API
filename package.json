    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.21.1",
    "nodemon": "^2.0.3",
    "pg": "^8.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
